<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Larvae -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>BMT_BovineBeetleLarvae</defName>
		<label>bovine grub</label>
		<description>A large beetle, commonly found in dark, damp places. Milkable.\n\nA large beetle originally from Caveworlds, this bug is often kept in farms for its production of a milk-like substance.</description>
		<statBases>
			<MoveSpeed>0.4</MoveSpeed>
			<LeatherAmount>0</LeatherAmount>
			<MarketValue>100</MarketValue>
			<ComfyTemperatureMin>-25</ComfyTemperatureMin>
			<ComfyTemperatureMax>60</ComfyTemperatureMax>
			<ToxicSensitivity>0</ToxicSensitivity>
		</statBases>
		<race>
			<hasGenders>false</hasGenders>
			<fleshType>Insectoid</fleshType>
			<bloodDef>Filth_BloodInsect</bloodDef>
			<meatColor>(160,168,139)</meatColor>
			<meatMarketValue>0.5</meatMarketValue>
			<useMeatFrom>Megaspider</useMeatFrom>
			<baseBodySize>1.5</baseBodySize>
			<baseHealthScale>0.8</baseHealthScale>
			<baseHungerRate>0.5</baseHungerRate>
			<foodType>OmnivoreRoughAnimal,AnimalProduct</foodType>
			<wildness>0.15</wildness>
			<trainability>None</trainability>
			<lifeExpectancy>0.5</lifeExpectancy>
			<manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.40</manhunterOnDamageChance>
			<body>BeetleLike</body>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
			<lifeStageAges>
				<li>
					<def>BMT_InsectLarvalFirst</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>BMT_InsectLarvalSecound</def>
					<minAge>0.05</minAge>
				</li>
				<li>
					<def>BMT_InsectLarvalThird</def>
					<minAge>0.1</minAge>
					<soundWounded>Pawn_Megascarab_Wounded</soundWounded>
					<soundDeath>Pawn_Megascarab_Death</soundDeath>
					<soundCall>Pawn_Megascarab_Call</soundCall>
					<soundAngry>Pawn_Megascarab_Angry</soundAngry>
				</li>
			</lifeStageAges>
		</race>
		<ingredient>
			<mergeCompatibilityTags>
				<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeat</li>
			</mergeCompatibilityTags>
		</ingredient>
		<uiIconScale>1.5</uiIconScale>
		<comps>
			<li Class="AnimalBehaviours.CompProperties_Metamorphosis">
				<timeInYears>0.15</timeInYears>
				<pawnToTurnInto>BMT_BovineBeetlePupa</pawnToTurnInto>
			</li>
		</comps>
		<tools>
			<li>
				<label>mandibles</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>4</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3.5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.1</chanceFactor>
			</li>
		</tools>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalInsect</li>
		</tradeTags>
	</ThingDef>
