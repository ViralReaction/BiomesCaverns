	<!-- Egg -->
	<ThingDef ParentName="EggFertBase">
		<defName>BMT_EggBovineBeetleFertilized</defName>
		<label>bovine beetle egg (fert.)</label>
		<description>A bovine beetle egg. If all goes well, it should hatch into a baby bovine beetle. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
			<color>(228,236,213)</color>
			<drawSize>1.3</drawSize>
		</graphicData>
		<statBases>
			<MarketValue>40.0</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>9</hatcherDaystoHatch>
				<hatcherPawn>BMT_BovineBeetleLarvae</hatcherPawn>
			</li>
		</comps>
	</ThingDef>
