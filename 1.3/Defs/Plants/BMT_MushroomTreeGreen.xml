<?xml version="1.0" encoding="utf-8" ?>
<Defs>	
	
	
    <ThingDef ParentName="BMT_CaveTreeBase">
        <defName>BMT_MushroomTreeGreen</defName>
        <label>green mushroom tree</label> 
        <description>A strong green mushroom tree. Slow-growing, but this woody fungus is very strong.</description>
        <graphicData>
            <texPath>BMT_Caverns/Things/Plant/MushroomTreeGreen</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <statBases>
            <Beauty>2</Beauty>
        </statBases>
        <plant>
            <wildClusterRadius>3</wildClusterRadius>
            <growDays>30</growDays>
            <dieIfLeafless>true</dieIfLeafless>
            <harvestWork>1000</harvestWork>
            <harvestYield>36</harvestYield>
			<wildOrder>1</wildOrder>
            <neverBlightable>true</neverBlightable>
        </plant>
    </ThingDef>
</Defs>