<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Caverns</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Royalty</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_TreeAnima"]</xpath>
						<value>
							<modExtensions>
								<li Class="BiomesCore.DefModExtensions.Plant_GraphicPerBiome">
									<biomes>
										<li>BMT_FungalForest</li>
									</biomes>
									<graphicPaths>
										<li>BMT_Caverns/Things/Plant/AnimaShroom/AnimaShroom</li>
									</graphicPaths>
								</li>
							</modExtensions>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_GrassAnima"]</xpath>
						<value>
							<modExtensions>
								<li Class="BiomesCore.DefModExtensions.Plant_GraphicPerBiome">
									<biomes>
										<li>BMT_FungalForest</li>
									</biomes>
									<graphicPaths>
										<li>BMT_Caverns/Things/Plant/AnimaShroom/AnimaShroomGrass</li>
									</graphicPaths>
								</li>
							</modExtensions>
						</value>
					</li>
				</operations>
			</match>
		</match>
	</Operation>
</Patch>