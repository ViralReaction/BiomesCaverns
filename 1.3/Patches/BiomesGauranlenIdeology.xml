<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Caverns</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Ideology</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]</xpath>
						<value>
							<modExtensions>
								<li Class="BiomesCore.DefModExtensions.Plant_GraphicPerBiome">
									<biomes>
										<li>BMT_FungalForest</li>
									</biomes>
									<graphicPaths>
										<li>BMT_Caverns/Things/Plant/GauranlenMushroom/GauranlenMushroomTree</li>
									</graphicPaths>
								</li>
							</modExtensions>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_MossGauranlen"]</xpath>
						<value>
							<modExtensions>
								<li Class="BiomesCore.DefModExtensions.Plant_GraphicPerBiome">
									<biomes>
										<li>BMT_FungalForest</li>
									</biomes>
									<graphicPaths>
										<li>BMT_Caverns/Things/Plant/GauranlenMushroom/GauranlenMushroomCluster</li>
									</graphicPaths>
								</li>
							</modExtensions>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_PodGauranlen"]</xpath>
						<value>
							<modExtensions>
								<li Class="BiomesCore.DefModExtensions.Plant_GraphicPerBiome">
									<biomes>
										<li>BMT_FungalForest</li>
									</biomes>
									<graphicPaths>
										<li>BMT_Caverns/Things/Plant/GauranlenMushroom/GauranlenMushroomPod</li>
									</graphicPaths>
								</li>
							</modExtensions>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]/plant</xpath>
						<value>
							<interferesWithRoof>false</interferesWithRoof>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_MossGauranlen"]/plant</xpath>
						<value>
							<interferesWithRoof>false</interferesWithRoof>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Plant_PodGauranlen"]/plant</xpath>
						<value>
							<interferesWithRoof>false</interferesWithRoof>
						</value>
					</li>
				</operations>
			</match>
		</match>
	</Operation>
</Patch>