<?xml version="1.0" encoding="utf-8" ?>
<Defs>	
	
	
    <ThingDef ParentName="BMT_CaveTreeBase">
        <defName>BMT_ImperialYellow</defName>
        <label>imperial yellow</label>
        <description>A strong yellow mushroom tree. Slow-growing, but this woody fungus is very strong and attractive.</description>
        <graphicData>
            <texPath>BMT_Caverns/Things/Plant/ImperialYellow</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <statBases>
            <Beauty>4</Beauty>
        </statBases>
        <plant>
            <wildClusterRadius>2</wildClusterRadius>
            <growDays>30</growDays>
            <dieIfLeafless>true</dieIfLeafless>
            <harvestWork>1000</harvestWork>
            <harvestYield>36</harvestYield>
			<wildOrder>1</wildOrder>
            <neverBlightable>true</neverBlightable>
        </plant>
    </ThingDef>
</Defs>